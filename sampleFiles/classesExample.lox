class Student{

	init(name, nickname){
		this.name = name;
		this.nickname = nickname;
	}

	toString(){
		return "<Student: " + this.name + ", " + this.nickname + " >";
	}
}

fun show_info(){

	print supreme.toString();
	print anna.toString();
}

